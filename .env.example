# Environment Variables Example
# Copy this file to .env and update the values as needed

# Application Environment
# Options: development, production
NODE_ENV=development

# Enable development features
# Set to false in production
VITE_DEV_MODE=true

# Maximum file size for JSON upload (in MB)
VITE_MAX_FILE_SIZE=10

# Session storage key prefix
VITE_STORAGE_PREFIX=mes_duplication_tool_

# Maximum number of recent files to store
VITE_MAX_RECENT_FILES=3

# Enable performance monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=false

# GitHub repository URL (for issue reporting)
VITE_GITHUB_REPO=https://github.com/yashasvi03/duplication-tool
